//ADDITION OF TWO MATRIX (ONE DIMENSIONAL)
#include<iostream>
using namespace std;
int main()
{int a[5],b[5],c[5],i,n;
cout<<"enter elements";
cin>>n;
for(i=0;i<5;i++)
{cin>>a[i];
}
cout<<"enter elements";
for(i=0;i<5;i++)
{cin>>b[i];
}
for(i=0;i<5;i++)
{c[i]=a[i]+b[i];
}
for(i=0;i<5;i++)
{

cout<<c[i]<<" ";
}
}

